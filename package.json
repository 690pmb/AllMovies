    "@angular-devkit/build-angular": "~0.1102.19",
    "@angular/cli": "~11.2",
    "@angular/language-service": "~11.2",
    "@types/crypto-js": "4.1.3",
    "@types/jasmine": "3.10.15",
    "@types/jasminewd2": "2.0.12",
    "@types/jquery": "3.5.32",
    "@types/node": "12.20.55",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "codelyzer": "6.0.2",
    "eslint": "8.7.0",
    "gts": "3.1.1",
    "husky": "7.0.4",
    "jasmine-core": "3.99.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.2",
