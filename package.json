  "devDependencies": {
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.17",
    "@angular/language-service": "~12.2.17",
    "@types/crypto-js": "4.2.2",
    "@types/jasmine": "3.10.18",
    "@types/jasminewd2": "2.0.13",
    "@types/jquery": "3.5.32",
    "@types/node": "14.14.31",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "codelyzer": "6.0.2",
    "eslint": "8.57.1",
    "gts": "3.1.1",
    "husky": "7.0.4",
    "jasmine-core": "3.99.1",
