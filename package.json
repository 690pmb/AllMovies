{
  "name": "all-movies",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4242",
    "mobile": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "run-s lint:**",
    "lint:ts": "gts lint",
    "lint:stylelint": "stylelint \"**/*.scss\" verbose",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/stats.json",
    "clean": "gts clean",
    "compile": "tsc --noEmit",
    "fix": "run-s fix:*",
    "fix:format": "prettier --ignore-unknown --write .",
    "fix:stylelint": "stylelint --fix \"**/*.scss\" verbose",
    "fix:fix": "gts fix",
    "prepare": "run-s prepare:*",
    "prepare:husky": "husky install",
    "prepare:compile": "npm run compile",
    "posttest": "npm run lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.17",
    "@angular/cdk": "~12.2.13",
    "@angular/common": "~12.2.17",
    "@angular/compiler": "~12.2.17",
    "@angular/core": "~12.2.17",
    "@angular/forms": "~12.2.17",
    "@angular/localize": "~12.2.17",
    "@angular/material": "~12.2.13",
    "@angular/platform-browser": "~12.2.17",
    "@angular/platform-browser-dynamic": "~12.2.17",
    "@angular-devkit/build-angular": "~19.1.5",
    "@angular/router": "~12.2.17",
    "@fortawesome/angular-fontawesome": "~0.9.0",
    "@fortawesome/fontawesome-svg-core": "~1.3",
    "@fortawesome/free-brands-svg-icons": "~5.15",
    "@fortawesome/free-regular-svg-icons": "~5.15",
    "@fortawesome/free-solid-svg-icons": "~5.15",
    "@ng-bootstrap/ng-bootstrap": "10.0.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "allorigins": "2.0.2",
    "bootstrap": "4.6.2",
    "crypto-js": "4.2.0",
    "dropbox": "4.0.30",
    "flag-icons": "7.2.3",
    "jquery": "3.7.1",
    "jsrsasign": "11.1.0",
    "jwt-decode": "3.1.2",
    "moment-mini-ts": "2.20.1",
    "ng2-nouislider": "1.8.3",
    "ngx-color-picker": "~11.0",
    "ngx-pinch-zoom": "2.6.2",
    "ngx-swiper-wrapper": "10.0.0",
    "nouislider": "15.8.1",
    "popper.js": "1.16.1",
    "rxjs": "6.6.7",
    "swiper": "^6.8.4",
    "zone.js": "0.11.8"
