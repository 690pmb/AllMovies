    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "26.0.0",
    "ts-node": "10.9.2",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "4.10.2"
  },
  "lint-staged": {
    "*.ts": "eslint --no-ignore --max-warnings 0 --fix",
    "*": "prettier --ignore-unknown --write",
    "*.scss": "stylelint --fix"
  }
