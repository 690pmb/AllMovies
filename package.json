    "@angular/core": "~11.2",
    "@angular/forms": "~11.2",
    "@angular/localize": "~11.2",
    "@angular/material": "~11.2",
    "@angular/platform-browser": "~11.2",
    "@angular/platform-browser-dynamic": "~11.2",
    "@angular/router": "~11.2",
    "@fortawesome/angular-fontawesome": "0.8.2",
    "@fortawesome/fontawesome-svg-core": "~1.3",
    "@fortawesome/free-brands-svg-icons": "~5.15",
    "@fortawesome/free-regular-svg-icons": "~5.15",
    "@fortawesome/free-solid-svg-icons": "~5.15",
    "@ng-bootstrap/ng-bootstrap": "9.1.3",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "allorigins": "2.0.2",
    "bootstrap": "4.6.1",
    "crypto-js": "4.1.1",
    "dropbox": "4.0.30",
    "flag-icons": "6.1.1",
    "jquery": "3.7.1",
    "jsrsasign": "10.5.25",
    "jwt-decode": "3.1.2",
    "moment-mini-ts": "2.20.1",
    "ng2-nouislider": "1.8.3",
    "ngx-color-picker": "~11.0",
    "ngx-pinch-zoom": "2.6.2",
    "ngx-swiper-wrapper": "10.0.0",
    "nouislider": "15.5.1",
