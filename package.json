    "typescript": "4.0.8",
    "webpack-bundle-analyzer": "4.5.0"
  },
  "lint-staged": {
    "*.ts": "eslint --no-ignore --max-warnings 0 --fix",
    "*": "prettier --ignore-unknown --write",
    "*.scss": "stylelint --fix"
  }
