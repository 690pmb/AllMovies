{
  "name": "all-movies",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4242",
    "mobile": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "run-p lint:**",
    "lint:ts": "gts lint",
    "lint:stylelint": "stylelint --syntax scss \"src/**/*.scss\" verbose",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/stats.json",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "run-s fix:*",
    "fix:format": "prettier --ignore-unknown --write .",
    "fix:stylelint": "stylelint --fix --syntax scss \"src/**/*.scss\" verbose",
    "fix:fix": "gts fix",
    "prepare": "run-s prepare:*",
    "prepare:husky": "husky install",
    "prepare:compile": "npm run compile",
    "posttest": "npm run lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2",
    "@angular/cdk": "~11.2",
    "@angular/common": "~11.2",
    "@angular/compiler": "~11.2",
